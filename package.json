{
  "name": "orcaclubpro",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.15",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "NODE_ENV=production next build",
    "build:with-payload": "bun run build:payload && bun run build:next",
    "build:payload": "bun run payload:generate && bun run payload:migrate",
    "build:next": "next build",
    "build:production": "NODE_ENV=production bun run build:payload && NODE_ENV=production bun run build:next",
    "build:comprehensive": "bun run scripts/build.js",
    "build:prod": "NODE_ENV=production bun run scripts/build.js",
    "start": "next start",
    "lint": "next lint",
    "bun:dev": "bun --bun run next dev --turbopack",
    "bun:build": "bun run build:payload && bun --bun run next build",
    "bun:build:production": "NODE_ENV=production bun run build:payload && NODE_ENV=production bun --bun run next build",
    "bun:start": "bun --bun run next start",
    "bun:lint": "bun --bun run next lint",
    "bun:dev:safe": "bun run next dev --turbopack",
    "bun:build:safe": "bun run build",
    "bun:start:safe": "bun run next start",
    "payload:generate": "payload generate:types",
    "payload:migrate": "payload migrate",
    "payload:migrate:create": "payload migrate:create",
    "clean": "rm -rf .next out node_modules/.cache",
    "build:clean": "bun run clean && bun run build",
    "deploy:build": "bun run clean && bun run build:production"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@libsql/darwin-arm64": "^0.5.22",
    "@payloadcms/db-mongodb": "3.44.0",
    "@payloadcms/db-sqlite": "3.44.0",
    "@payloadcms/next": "3.44.0",
    "@payloadcms/richtext-lexical": "3.44.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "googleapis": "^168.0.0",
    "graphql": "^16.12.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "payload": "3.44.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "recharts": "^2.15.4",
    "resend": "^6.6.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/node": "^20.19.26",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.6",
    "typescript": "^5.9.3"
  }
}
